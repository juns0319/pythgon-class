# try_count =10

answer =[1,3,5]
answer1 =[]
count =0
chance =10

guess =input().split(",")   #map함수 활용으로 편하게 변환 가능 예:a =(map(int,input().split()))
guess =[int (i) for i in guess]
for i in range(3):
    for j in range(3):
        if answer[i] ==guess[j]:
            if i ==j:
                answer1.append("strike")
                break
            else:
                answer1.append("ball")
                break
        else:
            answer1.append("foul")
            break
print(answer1)
